<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.java.ssm.mapper.yangfeimapper.YFMarketReturnMapper" >
  <resultMap id="BaseResultMap" type="org.java.ssm.entity.MarketReturn" >
    <id column="return_id" property="returnId" jdbcType="VARCHAR" />
    <result column="unit_id" property="unitId" jdbcType="INTEGER" />
    <result column="emp_id" property="empId" jdbcType="INTEGER" />
    <result column="return_money" property="returnMoney" jdbcType="DOUBLE" />
    <result column="return_time" property="returnTime" jdbcType="DATE" />
    <result column="return_statu" property="returnStatu" jdbcType="VARCHAR" />
  </resultMap>
 
<!-- 查询出所有的单号 --> 
<select id="getAllBills" resultType="String">
  select mr.return_id from market_return mr
</select> 
 
</mapper>