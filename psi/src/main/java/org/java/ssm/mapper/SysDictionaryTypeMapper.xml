<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.java.ssm.mapper.SysDictionaryTypeMapper">
	<!-- 根据数据字典的code字段来查询相关的结果集 -->
	<select id="findSysDictionaryTypeByCode" parameterType="String" resultType="java.util.HashMap">
		SELECT * FROM sys_dictionary_type  where code=#{_parameter}
	</select>


</mapper>
